{
  "name": "graphql-todo",
  "version": "1.0.0",
  "description": "learning graphql with prisma inside docker container",
  "type": "module",
  "main": "index.js",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "scripts": {
    "prisma:generate": "pnpm prisma generate",
    "prisma:migrate": "pnpm prisma migrate deploy",
    "build": "pnpm prisma:generate && tsc",
    "start": "pnpm build && pnpm prisma:migrate && node dist/index.js",
    "dev": "pnpm prisma migrate dev && nodemon --watch .env --watch src --exec \"node --loader ts-node/esm\" src/index.ts"
  },
  "keywords": [
    "graphql",
    "postgresql",
    "docker",
    "docker-compose",
    "graphql-api",
    "apollo-server"
  ],
  "author": "mikiyas-girma",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@prisma/client": "^6.13.0",
    "body-parser": "^2.2.0",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^16.0.2",
    "multer": "^2.0.2",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/graphql-upload": "^17.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.24",
    "esbuild": "^0.20.1",
    "nodemon": "^3.1.0",
    "prisma": "^6.13.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@10.14.0"
}
